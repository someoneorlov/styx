{
    "family": "flyway-migration-${ENVIRONMENT}",
    "containerDefinitions": [
        {
            "name": "flyway-migration-${ENVIRONMENT}",
            "image": "381491949871.dkr.ecr.us-east-1.amazonaws.com/flyway-migrations-${ENVIRONMENT}",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "ENVIRONMENT",
                    "value": "${ENVIRONMENT}"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "AWS_DB_HOST",
                    "valueFrom": "${AWS_DB_HOST}"
                },
                {
                    "name": "AWS_DB_PORT",
                    "valueFrom": "${AWS_DB_PORT}"
                },
                {
                    "name": "AWS_DB_NAME",
                    "valueFrom": "${AWS_DB_NAME}"
                },
                {
                    "name": "AWS_DB_USER",
                    "valueFrom": "${AWS_DB_USER}"
                },
                {
                    "name": "AWS_DB_PASS",
                    "valueFrom": "${AWS_DB_PASS}"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/flyway-migration-${ENVIRONMENT}",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::381491949871:role/ecsFlywayTaskRole",
    "executionRoleArn": "arn:aws:iam::381491949871:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}