{
    "family": "raw-data-transfer-test",
    "containerDefinitions": [
        {
            "name": "raw-data-transfer-test",
            "image": "381491949871.dkr.ecr.us-east-1.amazonaws.com/raw-data-transfer-test",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "SSH_PORT_FORWARD",
                    "value": "8004:127.0.0.1:8004"
                },
                {
                    "name": "DATA_PROVIDER_API_URL",
                    "value": "http://localhost:8004"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "AWS_DB_HOST",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:rds-db-credentials/styx_nlp_database_test-7z3wGk:host::"
                },
                {
                    "name": "AWS_DB_NAME",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:rds-db-credentials/styx_nlp_database_test-7z3wGk:dbname::"
                },
                {
                    "name": "AWS_DB_PASS",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:rds-db-credentials/styx_nlp_database_test-7z3wGk:password::"
                },
                {
                    "name": "AWS_DB_PORT",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:rds-db-credentials/styx_nlp_database_test-7z3wGk:port::"
                },
                {
                    "name": "AWS_DB_USER",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:rds-db-credentials/styx_nlp_database_test-7z3wGk:username::"
                },
                {
                    "name": "SSH_CONFIG",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:styx-remote/ssh-cIibvr:SSH_CONFIG::"
                },
                {
                    "name": "SSH_PRIVATE_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:styx-remote/ssh-cIibvr:SSH_PRIVATE_KEY::"
                },
                {
                    "name": "SSH_PUBLIC_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:styx-remote/ssh-cIibvr:SSH_PUBLIC_KEY::"
                },
                {
                    "name": "SSH_REMOTE_CREDS",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:381491949871:secret:styx-remote/ssh-cIibvr:SSH_REMOTE_CREDS::"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/raw-data-transfer-test",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::381491949871:role/ecsDataTransferRole",
    "executionRoleArn": "arn:aws:iam::381491949871:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
