{
    "family": "raw-data-transfer-${ENVIRONMENT}",
    "containerDefinitions": [
        {
            "name": "raw-data-transfer-${ENVIRONMENT}",
            "image": "381491949871.dkr.ecr.us-east-1.amazonaws.com/raw-data-transfer-${ENVIRONMENT}",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "SSH_PORT_FORWARD",
                    "value": "${SSH_PORT_FORWARD}"
                },
                {
                    "name": "DATA_PROVIDER_API_URL",
                    "value": "${DATA_PROVIDER_API_URL}"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "AWS_DB_HOST",
                    "valueFrom": "${AWS_DB_HOST}"
                },
                {
                    "name": "AWS_DB_NAME",
                    "valueFrom": "${AWS_DB_NAME}"
                },
                {
                    "name": "AWS_DB_PASS",
                    "valueFrom": "${AWS_DB_PASS}"
                },
                {
                    "name": "AWS_DB_PORT",
                    "valueFrom": "${AWS_DB_PORT}"
                },
                {
                    "name": "AWS_DB_USER",
                    "valueFrom": "${AWS_DB_USER}"
                },
                {
                    "name": "SSH_CONFIG",
                    "valueFrom": "${SSH_CONFIG}"
                },
                {
                    "name": "SSH_PRIVATE_KEY",
                    "valueFrom": "${SSH_PRIVATE_KEY}"
                },
                {
                    "name": "SSH_PUBLIC_KEY",
                    "valueFrom": "${SSH_PUBLIC_KEY}"
                },
                {
                    "name": "SSH_REMOTE_CREDS",
                    "valueFrom": "${SSH_REMOTE_CREDS}"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/raw-data-transfer-${ENVIRONMENT}",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::381491949871:role/ecsDataTransferRole",
    "executionRoleArn": "arn:aws:iam::381491949871:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}